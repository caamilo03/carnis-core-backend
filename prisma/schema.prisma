generator client {
  provider = "prisma-client-js" 
}

datasource db {
  provider = "postgresql"
  
}



model User {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String
  role      Role     @default(CASHIER)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}

enum Role {
  ADMIN
  BUTCHER
  CASHIER
}